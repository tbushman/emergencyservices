#menu.row.tb-6-m0
	li.tb-2-m0.tb-1-m2
		//- input#mySearch.catchphrase(type="search", placeholder="search", @change="searchThis")
		a(href="#", title="Menu")
			img(src="/images/menu-alt-512.svg")

		div#dropdown.row
			a.drop.tb-11-m0#shelterwatch(href="/shelterwatch")
				span.ms-font-l Shelter Watch
			a.drop.tb-11-m0#contact(href="/") 
				span.ms-font-l Home
